@model Bio
@{
    ViewData["Title"] = "Index";
}

    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Bio Table</h4>
                    <div class="table-responsive">
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th>
                                        Address
                                    </th>
                                    <th>
                                        Setting
                                    </th>

                                </tr>
                            </thead>
                            <tbody>
                                    <tr>
                                        <td class="py-1">
                                            @Model.Address
                                        </td>
                                        <td>
                                            <a asp-controller="Bio" asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary"><i class="mdi mdi-delete-forever">Edit</i></a>
                                        </td>
                                    </tr>
                               

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

